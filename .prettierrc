{
  "tabWidth": 2,
  "printWidth": 80,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "react",
    "react-dom",
    "<THIRD_PARTY_MODULES>",
    "^components/(.*)$",
    "^constants/(.*)$",
    "^hooks/(.*)$",
    "^layouts/(.*)$",
    "^pages/(.*)$",
    "^store/(.*)$",
    "^types/(.*)$",
    "^ui/(.*)$",
    "^utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}